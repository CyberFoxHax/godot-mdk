script/source = "extends Node3D

func _ready():
	var viewport = SubViewport.new()
	viewport.size = Vector2(256, 256)
	viewport.render_target_update_mode = SubViewport.UPDATE_ONCE
	add_child(viewport)
	
	var camera = Camera3D.new()
	camera.position = Vector3(0, 0, 1)  # Closer to mesh
	camera.current = true
	#add_child(camera)
	viewport.add_child(camera)
	
	var shader_material = ShaderMaterial.new()
	shader_material.shader = load(\"res://shaders/MDKTextureConverter.gdshader\")
	
	var rect = ColorRect.new()
	rect.size = Vector2(256, 20)
	rect.material = shader_material
	viewport.add_child(rect)

	#var mesh_instance = MeshInstance3D.new()
	#mesh_instance.mesh = PlaneMesh.new()
	#mesh_instance.mesh.size = Vector2(2, 1)
	#mesh_instance.rotation_degrees = Vector3(90, 0, 0)  # Face camera
	#viewport.add_child(mesh_instance)
	#add_child(mesh_instance)
	
	await RenderingServer.frame_post_draw
	
	var img = viewport.get_texture().get_image()
	#img.save_png(\"user://test_output.png\")  # Debug save
	
	var texture = ImageTexture.create_from_image(img)
	var testermat = StandardMaterial3D.new()
	testermat.albedo_texture = texture
	testermat.shading_mode = BaseMaterial3D.SHADING_MODE_UNSHADED
	$MeshInstance3D.material_override = testermat
		
	#viewport.queue_free()
"

